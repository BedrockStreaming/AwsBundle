{
    "name" : "m6web/aws-bundle",
    "description" : "Bundle Client AWS",
    "autoload" : {
        "psr-0" : {
            "" : "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "require" : {
        "php" : ">=5.4",
        "aws/aws-sdk-php" : "~2.6",
        "symfony/dependency-injection": "^2.8 || ^3.0"
    },
    "require-dev" : {
        "atoum/atoum-bundle" : "dev-master",
        "m6web/coke" : "~2.1",
        "m6web/symfony2-coding-standard" : "~3.3"
    },
    "suggest" : {
        "doctrine/cache" : "1.3.0"
    }
}